-include $(TOPDIR)/sysconfig

## Compile and Make Option
CONFIG_DEBUG=y
CONFIG_DEBUG_LEVEL=2
CONFIG_OSD2_ONLY=y

# console enable
CONFIG_CONSOLE_ENABLE=y
CONFIG_SYS_UART_BAUD=38400

# WDT timeout (millisecond)
CONFIG_WATCHDOG_TIMEOUT=10000

# nor flash layout
CONFIG_CODE_NOR_LAYOUT="NOR=(64,0-31)"






